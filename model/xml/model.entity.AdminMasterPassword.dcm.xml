<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="model\entity\AdminMasterPassword" table="admin_master_password">
    <indexes>
      <index name="master_id" columns="master_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="masterId" type="integer" column="master_id" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">用户id</option>
      </options>
    </field>
    <field name="password" type="string" column="password" length="32" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="comment">用户密码</option>
      </options>
    </field>
    <field name="salt" type="string" column="salt" length="6" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="comment">盐值</option>
      </options>
    </field>
    <field name="status" type="boolean" column="status" nullable="false">
      <options>
        <option name="comment">状态 1：启用中 0：未启用</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="addTime" type="string" column="add_time" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">添加时间</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
