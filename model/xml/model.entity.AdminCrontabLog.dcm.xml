<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="model\entity\AdminCrontabLog" table="admin_crontab_log">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="pid" type="integer" column="pid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">进程id</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="text" type="text" column="text" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">日志信息</option>
      </options>
    </field>
    <field name="label" type="string" column="label" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">标签</option>
      </options>
    </field>
    <field name="addTime" type="string" column="add_time" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">添加时间</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
