<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="model\entity\AdminMasterGroup" table="admin_master_group">
    <unique-constraints>
      <unique-constraint name="name" columns="name"/>
    </unique-constraints>
    <id name="id" type="smallint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">分组名称</option>
        <option name="default"/>
      </options>
    </field>
    <field name="gNavigation" type="text" column="g_navigation" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">该用户组的能看到的导航</option>
      </options>
    </field>
    <field name="addTime" type="string" column="add_time" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
